{
  "manifest_version": 2,
  "name": "Guitar trainer",
  "description": "This extension allows you to manipulate youtube's video.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "iconoff.ico",
    "default_popup": "popup.html",
    "default_title": "Guitar trainer"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab"
  ]
}